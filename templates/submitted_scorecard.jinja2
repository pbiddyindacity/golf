{% extends 'base.jinja2' %}

{% block content %}
<head>
      <meta charset="utf-8">
      <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
      <title>Scorecard</title>
      <meta name="description" content="">
      <meta name="viewport" content="width=device-width">
      <link rel="stylesheet" href="{{ url_for ('static', filename='css/main.css') }}">


      <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
      <script src="../static/javascript/script.js"></script>
</head>

<div class="container">

  <header>
    <h1>Scorecard</h1>
    <p colspan="9" id="selected_course"><strong>{{ name }}</strong> in {{ city }}, {{ state }}  {{ zip_code }} on {{ new_round_date }}
    <br>
    <i>Slope: {{ slope }}</i>
    </p>

  </header>

  <span id="basic"></span>


  <section>
    <table class="responsive-table scorecard">
      <thead>
        <tr class="hole-number">
          <th scope="col"></th>
          <th data-title="1" scope="col">1</th>
          <th data-title="2" scope="col">2</th>
          <th data-title="3" scope="col">3</th>
          <th data-title="4" scope="col">4</th>
          <th data-title="5" scope="col">5</th>
          <th data-title="6" scope="col">6</th>
          <th data-title="7" scope="col">7</th>
          <th data-title="8" scope="col">8</th>
          <th data-title="9" scope="col">9</th>

          <th data-title="Out" scope="col">Out</th>

          <th data-title="10" scope="col">10</th>
          <th data-title="11" scope="col">11</th>
          <th data-title="12" scope="col">12</th>
          <th data-title="13" scope="col">13</th>
          <th data-title="14" scope="col">14</th>
          <th data-title="15" scope="col">15</th>
          <th data-title="16" scope="col">16</th>
          <th data-title="17" scope="col">17</th>
          <th data-title="18" scope="col">18</th>

          <th data-title="In" scope="col">In</th>

          <th data-title="Total" scope="col">Total</th>
        </tr>
      </thead>

      <tbody>
        <tr>
          <th data-type="player" class"rowheader">Distance</th>
          <td>{{ h1d }}</td>
          <td>{{ h2d }}</td>
          <td>{{ h3d }}</td>
          <td>{{ h4d }}</td>
          <td>{{ h5d }}</td>
          <td>{{ h6d }}</td>
          <td>{{ h7d }}</td>
          <td>{{ h8d }}</td>
          <td>{{ h9d }}</td>
          <td>{{ front_9_distance }}</td>

          <td>{{ h10d }}</td>
          <td>{{ h11d }}</td>
          <td>{{ h12d }}</td>
          <td>{{ h13d }}</td>
          <td>{{ h14d }}</td>
          <td>{{ h15d }}</td>
          <td>{{ h16d }}</td>
          <td>{{ h17d }}</td>
          <td>{{ h18d }}</td>
          <td>{{ back_9_distance }}</td>

          <td>{{ total_distance }}</td>
        </tr>

        <tr>
          <th data-type="player" class"rowheader">Handicap</th>
          <td>{{ h1h }}</td>
          <td>{{ h2h }}</td>
          <td>{{ h3h }}</td>
          <td>{{ h4h }}</td>
          <td>{{ h5h }}</td>
          <td>{{ h6h }}</td>
          <td>{{ h7h }}</td>
          <td>{{ h8h }}</td>
          <td>{{ h9h }}</td>
          <td>&nbsp;</td>

          <td>{{ h10h }}</td>
          <td>{{ h11h }}</td>
          <td>{{ h12h }}</td>
          <td>{{ h13h }}</td>
          <td>{{ h14h }}</td>
          <td>{{ h15h }}</td>
          <td>{{ h16h }}</td>
          <td>{{ h17h }}</td>
          <td>{{ h18h }}</td>
          <td>&nbsp;</td>

          <td>&nbsp;</td>
        </tr>

        <tr>
          <th data-type="player" class"rowheader">Par</th>
          <td>{{ h1p }}</td>
          <td>{{ h2p }}</td>
          <td>{{ h3p }}</td>
          <td>{{ h4p }}</td>
          <td>{{ h5p }}</td>
          <td>{{ h6p }}</td>
          <td>{{ h7p }}</td>
          <td>{{ h8p }}</td>
          <td>{{ h9p }}</td>
          <td>{{ front_9_par }}</td>

          <td>{{ h10p }}</td>
          <td>{{ h11p }}</td>
          <td>{{ h12p }}</td>
          <td>{{ h13p }}</td>
          <td>{{ h14p }}</td>
          <td>{{ h15p }}</td>
          <td>{{ h16p }}</td>
          <td>{{ h17p }}</td>
          <td>{{ h18p }}</td>
          <td>{{ back_9_par }}</td>

          <td>{{ total_par }}</td>
        </tr>

        <tr>

          <th data-type="player" class"rowheader">
            {{ p1_name }}
          </th>
          <td>
            <span class="{{hole_1_style}}">{{ p1h1 }}</span>
          </td>
          <td>
            <span class="{{hole_2_style}}">{{ p1h2 }}</span>
          </td>
          <td>
            <span class="{{hole_3_style}}">{{ p1h3 }}</span>
          </td>
          <td>
            <span class="{{hole_4_style}}">{{ p1h4 }}</span>
          </td>
          <td>
            <span class="{{hole_5_style}}">{{ p1h5 }}</span>
          </td>
          <td>
            <span class="{{hole_6_style}}">{{ p1h6 }}</span>
          </td>
          <td>
            <span class="{{hole_7_style}}">{{ p1h7 }}</span>
          </td>
          <td>
            <span class="{{hole_8_style}}">{{ p1h8 }}</span>
          </td>
          <td>
            <span class="{{hole_9_style}}">{{ p1h9 }}</span>
          </td>
          <td><span>{{ p1f9 }}</span></td>

          <td>
            <span class="{{hole_10_style}}">{{ p1h10 }}</span>
          </td>
          <td>
            <span class="{{hole_11_style}}">{{ p1h11 }}</span>
          </td>
          <td>
            <span class="{{hole_12_style}}">{{ p1h12 }}</span>
          </td>
          <td>
            <span class="{{hole_13_style}}">{{ p1h13 }}</span>
          </td>
          <td>
            <span class="{{hole_14_style}}">{{ p1h14 }}</span>
          </td>
          <td>
            <span class="{{hole_15_style}}">{{ p1h15 }}</span>
          </td>
          <td>
            <span class="{{hole_16_style}}">{{ p1h16 }}</span>
          </td>
          <td>
            <span class="{{hole_17_style}}">{{ p1h17 }}</span>
          </td>
          <td>
            <span class="{{hole_18_style}}">{{ p1h18 }}</span>
          </td>
          <td><span>{{ p1b9 }}</span></td>

          <td><span>{{ p1_total_score }}</span></td>
        </tr>

      </tbody>

      <tfoot>

      </tfoot>
    </table>
  </section>
  </div>
  </body>
{% endblock %}